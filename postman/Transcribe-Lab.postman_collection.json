          "mode": "raw",
          "raw": "{\n  \"mediaUri\": \"s3://YOUR_BUCKET/transcribe-lab/input/sample.mp3\",\n  \"mediaFormat\": \"mp3\",\n  \"languageCode\": \"ko-KR\",\n  \"identifyLanguage\": false,\n  \"enableSpeakerLabels\": false,\n  \"maxSpeakerLabels\": 2\n}\n"
        },
        "url": {
          "raw": "{{BASE_URL}}/jobs",
          "host": [
            "{{BASE_URL}}"
          ],
          "path": [
            "jobs"
          ]
        }
      }
    },
    {
      "name": "List Jobs",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{BASE_URL}}/jobs",
          "host": [
            "{{BASE_URL}}"
          ],
          "path": [
            "jobs"
          ]
        }
      }
    },
    {
      "name": "Get Job (by name)",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{BASE_URL}}/jobs/{{JOB_NAME}}",
          "host": [
            "{{BASE_URL}}"
          ],
          "path": [
            "jobs",
            "{{JOB_NAME}}"
          ]
        }
      }
    },
    {
      "name": "Stop Job (by name)",
      "request": {
        "method": "POST",
        "header": [],
