          "raw": "{{BASE_URL}}/jobs/{{JOB_NAME}}/stop",
          "host": [
            "{{BASE_URL}}"
          ],
          "path": [
            "jobs",
            "{{JOB_NAME}}",
            "stop"
          ]
        }
      }
    },
    {
      "name": "Delete Job (by name)",
      "request": {
        "method": "DELETE",
        "header": [],
        "url": {
          "raw": "{{BASE_URL}}/jobs/{{JOB_NAME}}",
          "host": [
            "{{BASE_URL}}"
          ],
          "path": [
            "jobs",
            "{{JOB_NAME}}"
          ]
        }
      }
    },
    {
      "name": "Fetch Result JSON (proxy)",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{BASE_URL}}/result?url={{RESULT_URL}}",
          "host": [
            "{{BASE_URL}}"
          ],
          "path": [
            "result"
          ],
          "query": [
            {
              "key": "url",
              "value": "{{RESULT_URL}}"
            }
          ]
        }
      }
    }
  ]
